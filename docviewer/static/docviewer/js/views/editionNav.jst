<div class="docviewer-historyLink" id="<%= date_string %>">
  <span class="docviewer-trigger">
    <a href="#" class="docviewer-navEditionTimestamp" title="<%= comment %>"><%= date_string_formatted %></a>
    <a href="/accounts/<%= author %>" class="docviewer-navEditionUser">(@<%= author %>)</a>
    <span class="docviewer-remove docviewer-editHidden" title="Remove this version" style="top:2px;"></span>
  </span>
</div>
