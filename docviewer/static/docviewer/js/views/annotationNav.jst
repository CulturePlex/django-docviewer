<div class="docviewer-annotationMarker" id="docviewer-annotationMarker-<%= id %>">
  <span class="docviewer-trigger">
    <span class="docviewer-navAnnotationTitle"><%= title %></span>
    <span class="docviewer-navAnnotationUser">(<a href="<%= user_url %>" class="docviewer-navAnnotationUser">@<%= author %></a>)</span>
    <span class="docviewer-navPageNumber">p.<%= page %></span>
  </span>
</div>
